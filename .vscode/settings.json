{
  "deno.enable": true,
  "deno.lint": true,
  "deno.importMap": "./import_map.json",
  "deno.config": "./deno.json",
  "editor.defaultFormatter": "denoland.vscode-deno",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.organizeImports": true,
    "source.fixAll": true
  }
}
